
{
  "name": "Vibe Check",
  "version": "3.0",
  "description": "What starts here changes the wurlD",
  "manifest_version": 2,

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["https://*/*"],
      "js": ["popup.js"]
    }
  ]


}